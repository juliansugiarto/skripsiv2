				.col-lg-2.no-padding-lr
					.hl-box
						h6.hl__sales-currency = "IDR"
						h4.hl__sales.bronze-sales
                        h6.hl__desc--bronze = "Bronze Sales"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@bronze_sales_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
						h6.hl__sales-currency = "IDR"
						h4.hl__sales.silver-sales
                        h6.hl__desc--silver = "Silver Sales"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@silver_sales_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
						h6.hl__sales-currency = "IDR"
						h4.hl__sales.gold-sales
                        h6.hl__desc--gold = "Gold Sales"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@gold_sales_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
				.col-lg-2.no-padding-lr
					.hl-box


          


					


