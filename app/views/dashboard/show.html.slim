#wrapper.toggled
	/! Sidebar
	#sidebar-wrapper
		ul.sidebar-nav
			li.sidebar-brand
				a href="#" 
					| Start Bootstrap
			li
				= link_to "RT Dashboard", dashboard_show_path
			li
				= link_to "Overview", dashboard_ga_show_path
			li
				a href="#"  Overview
			li
				a href="#"  Events
			li
				a href="#"  About
			li
				a href="#"  Services
			li
				a href="#"  Contact
				
	#page-content-wrapper
		.container-fluid.no-padding-r
			.row
				.col-lg-2
				  a#menu-toggle.btn.btn-default.icon-white href="#menu-toggle"
				  	= fa_icon "navicon"
				  = image_tag "sribu.svg", class: "img-responsive mt-50 ml-50", size: "100"
				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.ch-lead
						h6.hl__desc = "Lead + CH"
						h6.hl__desc--red = "Need Follow Up"
						.hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@yesterday_ch_lead}"

				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.ch-lead-fu 
						h6.hl__desc = "Lead + CH"
						h6.hl__desc--green = "Followed up"
						.hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@yesterday_ch_lead_fu}"

				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.contest-need-fu
						h6.hl__desc = "Contest"
						h6.hl__desc--red = "Need Follow Up"
						.hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.contest-paid
						h6.hl__desc = "Contest"
						h6.hl__desc--green = "PAID"
						.hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contest_paid_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.less-participate
						h6.hl__desc = "Contest"
						h6.hl__desc--red = "Less Participate"
						.hl-bottom-box
							/ h6.hl__bottom-desc = "Yesterday: #{@contest_less_last_month_count}"
            .row
                .col-lg-2.no-padding-lr
                    .hl-box
                        h4.hl__count.contest-open
                        h6.hl__desc--green = "Open"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_open_last_month_count}"

                .col-lg-2.no-padding-lr
                    .hl-box
                        h4.hl__count.contest-wp
                        h6.hl__desc--yellow = "Winner Pending"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_wp_last_month_count}"
                .col-lg-2.no-padding-lr
                    .hl-box
                        h4.hl__count.contest-ft
                        h6.hl__desc--blue = "File Transfer"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_ft_last_month_count}"
                .col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.contest-closed
                        h6.hl__desc--red = "Closed"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_closed_last_month_count}"
				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.new-ch-contest
						h6.hl__percentage.new-ch-contest
						h6.hl__desc--green = "New CH Contest"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@contests_new_ch_last_month_count}"

				.col-lg-2.no-padding-lr
					.hl-box
			.row
				.col-lg-2.no-padding-lr
					.hl-box
						h4.hl__count.saver
                        h6.hl__desc--saver = "Saver"
                        .hl-bottom-box
							h6.hl__bottom-desc = "Yesterday: #{@saver_last_month_count}"
				.col-lg-2.no-padding-lr
